{
  "name": "Boilerplate",
  "description": "Boilerplate: Node, Webpack, Isomorphic React, Less, Babel, Handlebars",
  "version": "1.0.0",
  "main": "./server.js",
  "author": "Fun Maker",
  "scripts": {
    "start": "cross-env NODE_ENV=development nodemon --exec babel-node --presets react,env --plugins transform-object-rest-spread server.js",
    "start:prod": "cd dist && cross-env NODE_ENV=production node server.bundle.js",
    "build:prod": "npm run build:prod-webpack && npm run build:prod-static && npm run build:prod-package",
    "build:prod-webpack": "cross-env NODE_ENV=production webpack --verbose --colors --display-error-details --config webpack.config.js",
    "build:prod-static": "cp -r static dist/",
    "build:prod-package": "node ./prepareDist.js"
  },
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.5",
    "@fortawesome/fontawesome-free-regular": "^5.0.9",
    "@fortawesome/fontawesome-free-solid": "^5.0.9",
    "@fortawesome/react-fontawesome": "0.0.18",
    "axios": "^0.17.1",
    "body-parser": "latest",
    "detect-node": "latest",
    "express": "^4.15.2",
    "express-handlebars": "^3.0.0",
    "express-promise-router": "^2.0.0",
    "fs-extra": "^5.0.0",
    "get-folder-size": "^1.0.1",
    "is-mp3": "^1.1.3",
    "mime-types": "^2.1.18",
    "node-vibrant": "^3.0.0",
    "prop-types": "^15.5.8",
    "query-string": "latest",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-draggable": "^3.0.5",
    "react-fontawesome": "^1.6.1",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.2.2",
    "read-chunk": "^2.1.0",
    "sharp": "^0.20.1",
    "webpack-node-externals": "^1.6.0",
    "ws": "^5.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-optional-chaining": "^7.0.0-beta.3",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.9",
    "extract-text-webpack-plugin": "^3.0.2",
    "handlebars-loader": "^1.6.0",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "nodemon": "^1.14.11",
    "react-hot-loader": "^3.1.3",
    "style-loader": "^0.19.1",
    "uglifyjs-webpack-plugin": "^1.1.6",
    "webpack": "^3.0.0",
    "webpack-dev-middleware": "^2.0.4",
    "webpack-dev-server": "^2.11.1",
    "webpack-hot-middleware": "^2.18.0"
  },
  "port": 3900
}
